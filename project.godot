config_version=5

[application]

config/name="EdgeShapers"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PlayerVariables="*res://00_Globals/player_variables.gd"
LevelManager="*res://00_Globals/level_manager.gd"
PlayerManager="*res://00_Globals/player_manager.gd"
PlayerHud="*res://GUI/player_hud/player_hud.tscn"
SceneTransition="*res://GUI/scene_transition/scene_transition.tscn"
PauseMenu="*res://GUI/pause_menu/pause_menu.tscn"
SaveManager="*res://00_Globals/save_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

